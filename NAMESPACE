S3method(print, summary.pIR)
S3method(print, summary.pIRList)

S3method(pIR, default)
S3method(pIR, formula)

S3method(vectorIndex, default)
S3method(vectorIndex, formula)

S3method(VI, default)
S3method(VI, formula)

